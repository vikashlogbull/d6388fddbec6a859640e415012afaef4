{
    "name": "sllh/php-cs-fixer-styleci-bridge",
    "description": "Auto configure PHP-CS-Fixer from StyleCI config file",
    "keywords": ["PHP-CS-Fixer", "StyleCI", "PSR", "PSR-1", "PSR-2", "PSR-4", "Symfony", "Laravel", "configuration"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3|^7.0",
        "symfony/yaml": "^2.3|^3.0",
        "symfony/config": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.3|^3.0",
        "symfony/console": "^2.3|^3.0",
        "composer/semver": "^1.0",
        "sllh/styleci-fixers": "^3.0|^4.0"
    },
    "require-dev": {
        "twig/twig": "^1.22",
        "matthiasnoback/symfony-config-test": "^1.2",
        "fabpot/php-cs-fixer": "^1.6.1",
        "satooshi/php-coveralls": "^1.0",
        "codeclimate/php-test-reporter": "^0.3",
        "symfony/phpunit-bridge": "^2.7.4|^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "SLLH\\StyleCIBridge\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\StyleCIBridge\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
